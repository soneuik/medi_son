<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.soneuik.medi_son">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">





        <activity android:name="com.soneuik.medi_son.logoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.soneuik.medi_son.HomeActivity" />
        <activity android:name="com.soneuik.medi_son.SleepActivity" />
        <activity android:name="com.soneuik.medi_son.MainActivity" />
        <activity android:name="com.soneuik.medi_son.MusicPlayerActivity" />


        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6769539245756212~9376524540" />
        
        <receiver android:name=".Services.NotificationActionsService"/>
        <service android:name=".Services.OnClearFromRecentService" />
    </application>

</manifest>